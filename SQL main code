-- 3. READING
-- WHERE
-- S1: Get all customers who have an Inactive account:  
SELECT * 
FROM Customer c
JOIN Account a ON c.CustomerID = a.CustomerID
WHERE a.AccountStatus = 'Inactive';
-- S2: Find transactions with an amount greater than ₱50,000:
SELECT * 
FROM Transaction
WHERE Amount > 10000;
-- S1: Get all customer who have an active accounts
SELECT * 
FROM Customer c
JOIN Account a ON c.CustomerID = a.CustomerID
WHERE a.AccountStatus = 'Active';
-- S4: Find Employee whose position is 'Manager':
SELECT * 
FROM Employee
WHERE Position = 'Manager';
-- S5: Get all loans with status 'Approved':
SELECT * 
FROM Loan
WHERE LoanType = 'Personal';

-- 2. IN Operator — Match Multiple Values
-- s1: Find customers with an account type of either 'Savings' or 'Checking':
SELECT * 
FROM Account
WHERE AccountType IN ('Savings');
-- s2: Find transactions type 'Withdrawal'
SELECT * 
FROM Transaction
WHERE TransactionType IN ('Withdrawal');
-- s3: Find accounts that are 'Active' or 'Dormant':
SELECT * 
FROM Account
WHERE AccountStatus IN ('Active');
-- s4:Find Bank Name 'MetroBank':
SELECT * 
FROM Bank
WHERE BankName IN ('MetroBank');
-- s5: Retrieve loans with types 'Personal', 'Auto', or 'Home':
SELECT * 
FROM Loan
WHERE LoanType IN ('Personal');

-- 3. AND Operator — Combine Conditions
-- s1: Get all customers with a balance greater than 10,000 and an active account:
SELECT * 
FROM Account
WHERE CurrentBalance > 10000 
  AND AccountStatus = 'Active';
-- s2: Find transactions over ₱5,000 and made from 'Savings' accounts:
SELECT * 
FROM Transaction t
JOIN Account a ON t.AccountID = a.AccountID
WHERE t.Amount > 5000 AND a.AccountType = 'Savings';
-- s3: Find all loans with a type of "Mortgage" and a loan amount greater than ₱200,000.
SELECT * 
FROM Loan
WHERE LoanType = 'Mortgage' 
  AND LoanAmount > 200000;
-- s4: Find transactions with an amount greater than ₱10,000 from active accounts.
SELECT * 
FROM Transaction t
JOIN Account a ON t.AccountID = a.AccountID
WHERE t.Amount > 10000 
AND a.AccountStatus = 'Active';
-- s5:Get loans with status 'Approved' and amount greater than ₱100,000:
SELECT * 
FROM Loan
WHERE Status = 'Active' AND LoanAmount > 100000;

-- 4. NOT Operator — Exclude Records
-- s1: get accounts that are not of type of checking
SELECT * 
FROM Account
WHERE AccountType NOT IN ('Checking');
-- s2; Get accounts that are not of type 'Savings':
 SELECT * 
FROM Account
WHERE AccountType NOT IN ('Savings');
-- s3: Find transactions not made in December:
SELECT * 
FROM Transaction
WHERE MONTH(TransactionDate) <> 12;
-- s4: Get all loans that are not of type 'Personal'
SELECT * 
FROM Loan
WHERE LoanType NOT IN ('Personal');
-- s5:Get accounts that are not 'InActive'
SELECT * 
FROM Account
WHERE AccountStatus NOT IN ('Active');

-- 5. COUNT Function — Aggregate Totals
-- Count the number of active customers:
SELECT COUNT(DISTINCT c.CustomerID) AS ActiveCustomers
FROM Customer c
JOIN Account a ON c.CustomerID = a.CustomerID
WHERE a.AccountStatus = 'Active';
-- Count the number of inactive customers:
SELECT COUNT(DISTINCT c.CustomerID) AS InactiveCustomers
FROM Customer c
JOIN Account a ON c.CustomerID = a.CustomerID
WHERE a.AccountStatus <> 'Active';
-- Count the total transactions in January:
SELECT COUNT(*) AS TotalTransactions
FROM Transaction
WHERE MONTH(TransactionDate) = 1;
-- Count the number of loans above ₱50,000:
SELECT COUNT(*) AS LoansAbove50K
FROM Loan
WHERE LoanAmount > 50000;
-- Count the number of customers from 'CEBU' branch:
SELECT COUNT(*) AS PersonalLoans
FROM Loan
WHERE LoanType = 'Personal';


-- 6. DISTINCT Keyword — Eliminate Duplicates
-- Get unique account types:
 SELECT DISTINCT AccountType
FROM Account;
-- List all distinct branches:
SELECT DISTINCT Bank
FROM Branch;
-- Find unique cities where customers live:
SELECT DISTINCT CustomerType
FROM Customer;
-- Get distinct transaction types:
SELECT DISTINCT TransactionType
FROM Transaction;
-- List distinct loan types:
SELECT DISTINCT LoanType
FROM Loan;

SELECT DISTINCT AccountStatus
FROM Account;

-- Updating Records
-- 4.1: Update a customer's phone number
UPDATE Customer
SET PhoneNumber = '09181234568'  -- New phone number
WHERE CustomerID = 1;  -- Specify the CustomerID of the customer whose phone number you want to update

-- 4.2: Change the account status to 'Active' or 'Inactive'
UPDATE Account
SET AccountStatus = 'Inactive'
WHERE AccountID = 2;  -- Specify the AccountID of the account you want to update

-- 4.3: Add interest to all savings accounts (3% of the current balance)
UPDATE Account
SET CurrentBalance = CurrentBalance + (CurrentBalance * 0.03)
WHERE AccountType = 'Savings';
SELECT * 
FROM Account
WHERE AccountType = 'Savings';

SET SQL_SAFE_UPDATES = 0;

-- 4.4: Change the account type for a specific account
UPDATE Account
SET AccountType = 'Checking'
WHERE AccountID = 1;  -- Specify the AccountID of the account you want to update

-- 5. deleting records
-- 5.1. Write an SQL query to delete a specific customer record based on CustomerID.
DELETE FROM Customer
WHERE CustomerID = 53;  -- Replace 1 with the actual CustomerID you want to delete
ALTER TABLE account
DROP FOREIGN KEY account_ibfk_1,
ADD CONSTRAINT account_ibfk_1
FOREIGN KEY (CustomerID) REFERENCES customer(CustomerID)
ON DELETE CASCADE;

-- 5.2. Write an SQL query to delete all accounts with a balance of zero.
DELETE FROM Account
WHERE CurrentBalance = 0;
SELECT * 
FROM Account
WHERE CurrentBalance = 0;

-- ADVANCE SCENARIOS
-- 6.1. Write an SQL query to find customers who do not have an account yet.
SELECT c.CustomerID, c.FirstName, c.LastName
FROM Customer c
LEFT JOIN Account a ON c.CustomerID = a.CustomerID
WHERE a.AccountID IS NULL;

-- 6.2. Write an SQL query to delete a customer and all associated accounts in one operation.
DELETE FROM account WHERE CustomerID = 52;
DELETE FROM customer WHERE CustomerID = 52;

-- VERIFYING IF NO RETURNING VALUES MEANS WE SUCCESSFULLY DELETED ID NUMBER 52
-- 1. Verify that the customer has been deleted
SELECT * 
FROM Customer
WHERE CustomerID = 52;  -- Replace with the actual CustomerID
-- 2. Verify that all associated accounts have been deleted
SELECT * 
FROM Account
WHERE CustomerID = 52;  -- Replace with the actual CustomerID
-- 3. Verify that all transactions associated with the customer have been deleted
SELECT * 
FROM Transaction
WHERE AccountID IN (SELECT AccountID FROM Account WHERE CustomerID = 52);  -- Replace with the actual CustomerID
-- 4. Verify that all loans associated with the customer have been deleted
SELECT * 
FROM Loan
WHERE CustomerID = 52;  -- Replace with the actual CustomerID

