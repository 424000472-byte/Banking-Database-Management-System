-- Create the database
CREATE DATABASE BankingSystem;
USE BankingSystem;

-- BANK ENTITY
CREATE TABLE Bank (
    Code VARCHAR(20) PRIMARY KEY,         -- Unique bank code
    BankName VARCHAR(100) NOT NULL,       -- Name of the bank
    Address VARCHAR(200)                  -- Bank address
);

-- BRANCH ENTITY
CREATE TABLE Branch (
    BranchID INT PRIMARY KEY AUTO_INCREMENT, -- Surrogate PK
    BranchName VARCHAR(100) NOT NULL,        -- Branch commercial name
    Address VARCHAR(200),                    -- Branch address
    BranchCode VARCHAR(100) UNIQUE NOT NULL, -- Internal branch code
    PhoneNumber VARCHAR(20),
    BankCode VARCHAR(20),                    -- FK to Bank
    FOREIGN KEY (BankCode) REFERENCES Bank(Code)
);

-- CUSTOMER ENTITY
CREATE TABLE Customer (
    CustomerID INT PRIMARY KEY AUTO_INCREMENT, -- Surrogate PK
    CustomerType VARCHAR(20),                  -- Regular, Premium, etc.
    LastName VARCHAR(100) NOT NULL,
    FirstName VARCHAR(100) NOT NULL,
    DateOfBirth DATE,
    Email VARCHAR(100) UNIQUE,
    PhoneNumber VARCHAR(20),
    Address VARCHAR(200)
);

-- EMPLOYEE ENTITY
CREATE TABLE Employee (
    EmployeeID INT PRIMARY KEY AUTO_INCREMENT, -- Surrogate PK
    Position VARCHAR(20),                      -- Teller, Manager, etc.
    LastName VARCHAR(100) NOT NULL,
    FirstName VARCHAR(100) NOT NULL,
    DateOfBirth DATE,
    Email VARCHAR(100) UNIQUE,
    PhoneNumber VARCHAR(20),
    Address VARCHAR(200),
    BranchID INT,                              -- FK to Branch
    FOREIGN KEY (BranchID) REFERENCES Branch(BranchID)
);

-- ACCOUNT ENTITY
CREATE TABLE Account (
    AccountID INT PRIMARY KEY AUTO_INCREMENT,  -- Surrogate PK
    AccountType VARCHAR(20),                   -- Savings, Checking, etc.
    AccountNumber VARCHAR(20) UNIQUE NOT NULL, -- Unique account number
    CurrentBalance DECIMAL(15,2) DEFAULT 0.00,
    DateOpened DATE NOT NULL,
    DateClosed DATE,
    AccountStatus VARCHAR(20) DEFAULT 'Active',
    CustomerID INT,                            -- FK to Customer
    BranchID INT,                              -- FK to Branch
    FOREIGN KEY (CustomerID) REFERENCES Customer(CustomerID),
    FOREIGN KEY (BranchID) REFERENCES Branch(BranchID)
);

-- TRANSACTION ENTITY
CREATE TABLE Transaction (
    TransactionID INT PRIMARY KEY AUTO_INCREMENT, -- Surrogate PK
    TransactionType VARCHAR(20),                  -- Deposit, Withdrawal, etc.
    Amount DECIMAL(15,2) NOT NULL,
    TransactionDate DATETIME NOT NULL,
    AccountID INT,                                -- FK to Account
    FOREIGN KEY (AccountID) REFERENCES Account(AccountID)
);

-- LOAN ENTITY
CREATE TABLE Loan (
    LoanID INT PRIMARY KEY AUTO_INCREMENT,   -- Surrogate PK
    LoanType VARCHAR(20),                    -- Personal, Mortgage, etc.
    LoanAmount DECIMAL(15,2) NOT NULL,
    InterestRate DECIMAL(5,2) NOT NULL,      -- Annual %
    Term INT NOT NULL,                       -- Duration in months
    StartDate DATE NOT NULL,
    EndDate DATE,
    Status VARCHAR(20) DEFAULT 'Active',
    CustomerID INT,                          -- FK to Customer
    BranchID INT,                            -- FK to Branch
    FOREIGN KEY (CustomerID) REFERENCES Customer(CustomerID),
    FOREIGN KEY (BranchID) REFERENCES Branch(BranchID)
);

-- LOAN PAYMENT ENTITY
CREATE TABLE LoanPayment (
    LoanPaymentID INT PRIMARY KEY AUTO_INCREMENT, -- Surrogate PK
    ScheduledPaymentDate DATE NOT NULL,
    PaymentAmount DECIMAL(15,2) NOT NULL,
    PrincipalAmount DECIMAL(15,2),
    InterestAmount DECIMAL(15,2),
    PaidAmount DECIMAL(15,2),
    PaidDate DATE,
    LoanID INT,                                   -- FK to Loan
    FOREIGN KEY (LoanID) REFERENCES Loan(LoanID)
);

-- View all banks
SELECT * FROM Bank;

-- View all branches
SELECT * FROM Branch;

-- View all customers
SELECT * FROM Customer;

-- View all employees
SELECT * FROM Employee;

-- View all accounts
SELECT * FROM Account;

-- View all transactions
SELECT * FROM Transaction;

-- View all loans
SELECT * FROM Loan;

-- View all loan payments
SELECT * FROM LoanPayment;

